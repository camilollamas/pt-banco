{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET", "POST"],
      "dest": "/"
    },
    {
      "src": "/creditos",
      "methods": ["GET", "POST"],
      "dest": "/creditos"
    },
    {
      "src": "/clientes/",
      "methods": ["GET", "POST"],
      "dest": "/clientes"
    },
    {
      "src": "/auth/login",
      "methods": ["GET", "POST"],
      "dest": "/auth/login"
    }
  ],
  "crons": [
    {
      "path": "/auth/login",
      "schedule": "0 10 * * *"
    }
  ]
}